{
    "base": {
        "model": "wdl",
        "embedding_dim": 128,
        "dataset": "criteotb",
        "max_ind_range": 40000000,
        "learning_rate": 1.0,
        "mini_batch_size": 2048,
        "print_freq": 2048,
        "test_freq": 102400,
        "data_path": "datasets/criteotb"
    },
    "full": {
        "tensor_board_filename": "board/wdl_criteotb/full"
    },
    "hash": {
        "compress_method": "hash",
        "tensor_board_filename": "board/wdl_criteotb/hash",
        "compress_rate": [
            0.05, 0.01, 0.002, 0.0005, 0.0001
        ]
    },
    "qr": {
        "compress_method": "qr",
        "tensor_board_filename": "board/wdl_criteotb/qr",
        "compress_rate": [
            0.05, 0.01, 0.002, 0.0005
        ]
    },
    "ada": {
        "compress_method": "ada",
        "tensor_board_filename": "board/wdl_criteotb/ada",
        "compress_rate": [
            0.05
        ]
    },
    "cafe": {
        "compress_method": "cafe",
        "tensor_board_filename": "board/wdl_criteotb/cafe",
        "compress_rate": [
            0.05, 0.01, 0.002, 0.0005, 0.0001
        ],
        "cafe_sketch_threshold": [
            30, 100, 500, 500, 500
        ],
        "cafe_hash_rate": [
            0.5, 0.3, 0.2, 0.1, 0.1
        ]
    }
}
